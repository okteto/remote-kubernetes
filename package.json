{
	"name": "okteto-vscode",
	"publisher": "okteto",
	"displayName": "okteto",
	"description": "Develop directly in Kubernetes with Okteto",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "okteto.up",
				"title": "Okteto: Start"
			},
			{
				"command": "okteto.down",
				"title": "Okteto: Stop"
			}
		],
		"configuration": {
			"title": "Okteto",
			"properties": {
				"okteto.binary": {
					"type": "string",
					"default": "",
					"description": "The path to the Okteto binary. When empty, it will use \"okteto\" on the path"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@kubernetes/client-node": "^0.10.2",
		"@types/command-exists": "^1.2.0",
		"@types/user-home": "^2.0.0",
		"@types/yaml": "^1.0.2",
		"command-exists": "^1.2.8",
		"execa": "^2.0.4",
		"fs": "^0.0.1-security",
		"ssh-config": "^2.0.0-beta.1",
		"tcp-ping": "^0.1.1",
		"user-home": "^2.0.0",
		"yaml": "^1.6.0"
	}
}
