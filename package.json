{
	"name": "remote-kubernetes",
	"publisher": "Okteto",
	"displayName": "Remote - Kubernetes",
	"description": "Remote Kubernetes Development in VS Code with Okteto",
	"keywords": [
		"kubernetes",
		"docker",
		"remote",
		"remote development",
		"ssh",
		"containers",
		"aks",
		"gke",
		"eks"
	],
	"version": "0.1.7",
	"icon": "media/icon.png",
	"author": {
		"name": "rberrelleza"
	},
	"preview": true,
	"homepage": "https://github.com/okteto/remote-kubernetes",
	"bugs": {
		"url": "https://github.com/okteto/remote-kubernetes/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/okteto/remote-kubernetes.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other",
		"Azure"
	],
	"activationEvents": [
		"onCommand:okteto.install",
		"onCommand:okteto.up",
		"onCommand:okteto.down",
		"onCommand:okteto.create"
	],
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "okteto.install",
				"title": "Okteto: Install"
			},
			{
				"command": "okteto.up",
				"title": "Okteto: Up"
			},
			{
				"command": "okteto.down",
				"title": "Okteto: Down"
			},
			{
				"command": "okteto.create",
				"title": "Okteto: Create manifest",
				"enablement": "workspaceFolderCount != 0"
			}
		],
		"configuration": {
			"title": "Okteto",
			"properties": {
				"okteto.binary": {
					"type": "string",
					"default": "",
					"description": "An absolute path to the Okteto executable. When empty, it will use \"okteto\" on the path"
				},
				"okteto.telemetry": {
					"type": "boolean",
					"default": true,
					"description": "Enable data collection"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode development",
		"watch": "webpack --mode development --watch",
		"package": "vsce package",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"publish": "vsce publish -p $VSCE_TOKEN"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"ts-loader": "^6.1.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vsce": "^1.67.1",
		"vscode-test": "^1.0.2",
		"webpack": "^4.40.2",
		"webpack-cli": "^3.3.8"
	},
	"dependencies": {
		"@kubernetes/client-node": "^0.10.2",
		"@types/command-exists": "^1.2.0",
		"@types/download": "^6.2.4",
		"@types/semver": "^6.0.2",
		"@types/user-home": "^2.0.0",
		"@types/yaml": "^1.0.2",
		"bufferutil": "^4.0.1",
		"command-exists": "^1.2.8",
		"download": "^7.1.0",
		"execa": "^2.0.4",
		"fs": "^0.0.1-security",
		"get-port": "^5.0.0",
		"mixpanel": "^0.10.2",
		"semver": "^6.3.0",
		"tcp-ping": "^0.1.1",
		"title-case": "^2.1.1",
		"user-home": "^2.0.0",
		"utf-8-validate": "^5.0.2",
		"yaml": "^1.6.0"
	}
}
